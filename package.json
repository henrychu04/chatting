    "@types/react-dom": "^19.1.6",
    "dotenv-cli": "^8.0.0",
    "drizzle-kit": "^0.31.4",
    "tailwindcss": "^4.1.4",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5.8.3",
    "vite": "^7.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.13.2"
  }
